<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/telemedicinaFragment">

    <fragment
        android:id="@+id/telemedicinaFragment"
        android:name="com.example.movildev.TelemedicinaFragment"
        android:label="TelemedicinaFragment" >
        <action
            android:id="@+id/action_telemedicinaFragment_to_salaFragment"
            app:destination="@id/salaFragment" />
    </fragment>
    <fragment
        android:id="@+id/salaFragment"
        android:name="com.example.movildev.SalaFragment"
        android:label="SalaFragment" >
        <action
            android:id="@+id/action_salaFragment_to_llamadaFragment"
            app:destination="@id/llamadaFragment" />
    </fragment>
    <fragment
        android:id="@+id/llamadaFragment"
        android:name="com.example.movildev.LlamadaFragment"
        android:label="LlamadaFragment" />
</navigation>