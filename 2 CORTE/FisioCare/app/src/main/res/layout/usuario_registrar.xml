<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        android:layout_width="412dp"
        android:layout_height="56dp"
        app:cardElevation="10dp"
        app:cardBackgroundColor="@color/fondo_3"
        tools:ignore="MissingConstraints">

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/fragmentContainerView"
            android:name="androidx.fragment.app.FragmentContainerView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@android:color/transparent"/>
    </androidx.cardview.widget.CardView>


    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentfondo"
        android:name="androidx.fragment.app.FragmentContainerView"
        android:layout_width="414dp"
        android:layout_height="608dp"
        android:layout_marginTop="60dp"
        android:background="@color/fondo_2"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-2dp" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentfondo"
        android:name="androidx.fragment.app.FragmentContainerView"
        android:layout_width="275dp"
        android:layout_height="334dp"
        android:alpha="0.3"
        android:background="@drawable/rounded_fragment2"
        android:elevation="10dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="125dp" />

    <TextView
        android:id="@+id/logo"
        android:layout_width="110dp"
        android:layout_height="26dp"
        android:elevation="10dp"
        android:fontFamily="serif"
        android:text="Usuario"
        android:textColor="@color/azul_main"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="18dp" />

    <ImageView
        android:id="@+id/logoImageView"
        android:layout_width="49dp"
        android:layout_height="40dp"
        android:background="@android:color/transparent"
        android:contentDescription="Logo"
        android:elevation="10dp"
        android:src="@drawable/ic_arrowback"
        app:tint="@color/azul_main"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="10dp" />


    <ImageView
        android:id="@+id/logoImageView"
        android:layout_width="33dp"
        android:layout_height="27dp"
        android:background="@android:color/transparent"
        android:contentDescription="Logo"
        android:elevation="10dp"
        android:src="@drawable/ic_user"
        android:tint="@color/azul_main"

        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="17dp" />


    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo de documento"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="219dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre y apellido"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="158dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Numero de documento"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="299dp" />
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fecha Nacimiento"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="364dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Correo ElectrÃ³nico"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="420dp" />

    <EditText
        android:layout_width="271dp"
        android:layout_height="42dp"
        android:hint="Nombre completo"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="177dp" />

    <EditText
        android:layout_width="143dp"
        android:layout_height="39dp"
        android:hint="Type"
        android:inputType="number"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="238dp" />

    <EditText
        android:layout_width="197dp"
        android:layout_height="39dp"
        android:hint="Value"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="325dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="197dp"
        android:layout_height="39dp"
        android:hint="dd/mm/aaaa"
        android:inputType="date"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="383dp" />
    <EditText
        android:id="@+id/ediText"
        android:layout_width="197dp"
        android:layout_height="39dp"
        android:hint="Correo Electronico"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="440dp" />

    <Button
        android:layout_width="86dp"
        android:layout_height="29dp"
        android:background="@drawable/button_rounded"
        android:elevation="10dp"
        android:text="Guardar"
        android:textColor="@color/white"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="504dp" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_marginBottom="4dp"
        android:elevation="20dp"
        android:background="?android:attr/windowBackground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/bottom_menu"
        tools:layout_editor_absoluteX="0dp" />
    <ImageView
        android:id="@+id/logocasapeque"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@android:color/transparent"
        android:elevation="30dp"
        android:src="@drawable/ic_home"
        android:tint="@color/azul_main"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="676dp"
        tools:ignore="MissingConstraints,UseAppTint" />
    <TextView
        android:id="@+id/InicioPeque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inicio"
        android:textColor="@color/azul_main"
        android:textSize="14dp"
        android:elevation="30dp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="705dp"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/logocitaspeque"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@android:color/transparent"
        android:elevation="30dp"
        android:src="@drawable/ic_calendar"
        android:tint="@color/azul_main"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="676dp"
        tools:ignore="MissingConstraints,UseAppTint" />
    <TextView
        android:id="@+id/citasPeque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Citas"
        android:textColor="@color/azul_main"
        android:textSize="14dp"
        android:elevation="30dp"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="705dp"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/logohistoriapeque"
        android:layout_width="30dp"
        android:layout_height="28dp"
        android:background="@android:color/transparent"
        android:elevation="30dp"
        android:src="@drawable/ic_hc"
        android:tint="@color/azul_main"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="679dp"
        tools:ignore="MissingConstraints,UseAppTint" />
    <TextView
        android:id="@+id/hcPeque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Historia"
        android:textColor="@color/azul_main"
        android:textSize="14dp"
        android:elevation="30dp"
        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="705dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/logoperfilpeque"
        android:layout_width="30dp"
        android:layout_height="28dp"
        android:background="@android:color/transparent"
        android:elevation="30dp"
        android:src="@drawable/ic_user"
        android:tint="@color/azul_main"
        tools:layout_editor_absoluteX="330dp"
        tools:layout_editor_absoluteY="679dp"
        tools:ignore="MissingConstraints,UseAppTint" />
    <TextView
        android:id="@+id/PerfilPeque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Perfil"
        android:textColor="@color/azul_main"
        android:textSize="14dp"
        android:elevation="30dp"
        tools:layout_editor_absoluteX="330dp"
        tools:layout_editor_absoluteY="705dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>