<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        android:layout_width="405dp"
        android:layout_height="51dp"
        android:background="@color/white"
        app:cardElevation="10dp"
        app:layout_constraintBottom_toTopOf="@id/bottomNavigationView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="0dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/logo"
        android:layout_width="100dp"
        android:layout_height="25dp"
        android:elevation="10dp"
        android:fontFamily="serif"
        android:text="FisioCare"
        android:textColor="@color/azul_main"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="18dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/flechaatras"
        android:layout_width="49dp"
        android:layout_height="40dp"
        android:background="@android:color/transparent"
        android:contentDescription="Logo"
        android:elevation="10dp"
        android:src="@drawable/ic_arrowback"
        android:tint="@color/azul_main"

        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="10dp" />


    <ImageView
        android:id="@+id/logofactura"
        android:layout_width="36dp"
        android:layout_height="30dp"
        android:background="@android:color/transparent"
        android:contentDescription="Logo"
        android:elevation="10dp"
        android:src="@drawable/hand_heart_svgrepo_com"

        app:tint="@color/azul_main"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="16dp"
        tools:ignore="MissingConstraints" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentfondo"
        android:name="androidx.fragment.app.FragmentContainerView"
        android:layout_width="410dp"
        android:layout_height="621dp"
        android:layout_marginTop="48dp"
        android:background="@color/fondo_main"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="1dp"
        tools:ignore="MissingConstraints" />




    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentContainerView"
        android:name="androidx.fragment.app.FragmentContainerView"
        android:layout_width="380dp"
        android:layout_height="550dp"
        android:alpha="0.8"
        android:background="@drawable/fraagment_border"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="80dp"
        tools:ignore="MissingConstraints" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentContainer"
        android:name="androidx.fragment.app.FragmentContainerView"
        android:layout_width="251dp"
        android:layout_height="250dp"
        android:alpha="0.8"
        android:background="@drawable/rounded_fragment"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="300dp"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/MenuUsuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Crear Usuario"
        android:textColor="@color/black"
        android:textSize="14dp"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="335dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:layout_width="73dp"
        android:layout_height="23dp"
        android:background="@drawable/button_rounded"
        android:elevation="10dp"
        android:text="Ir"
        android:textColor="@color/white"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="408dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/ConsultarUsuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Consultar Usuario"
        android:textColor="@color/black"
        android:textSize="14dp"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="408dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:layout_width="73dp"
        android:layout_height="23dp"
        android:background="@drawable/button_rounded"
        android:elevation="10dp"
        android:text="Ir"
        android:textColor="@color/white"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="335dp"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/EditarUsuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Editar Usuario"
        android:textColor="@color/black"
        android:textSize="14dp"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="485dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:layout_width="73dp"
        android:layout_height="23dp"
        android:background="@drawable/button_rounded"
        android:elevation="10dp"
        android:text="Ir"
        android:textColor="@color/white"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="485dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/Usuariotext"
        android:layout_width="233dp"
        android:layout_height="60dp"
        android:fontFamily="serif"
        android:text="Usuarios"
        android:textColor="@color/azul_main"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="118dp"
        tools:layout_editor_absoluteY="137dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/logousuariogrande"
        android:layout_width="63dp"
        android:layout_height="80dp"
        android:background="@android:color/transparent"
        android:elevation="10dp"
        android:src="@drawable/ic_user"
        android:tint="@color/azul_main"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="125dp"
        tools:ignore="MissingConstraints,UseAppTint" />



    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_marginBottom="4dp"
        android:elevation="20dp"
        android:background="?android:attr/windowBackground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/bottom_menu"
        tools:layout_editor_absoluteX="0dp" />
    <ImageView
        android:id="@+id/logocasapeque"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@android:color/transparent"
        android:elevation="30dp"
        android:src="@drawable/ic_home"
        android:tint="@color/azul_main"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="676dp"
        tools:ignore="MissingConstraints,UseAppTint" />
    <TextView
        android:id="@+id/InicioPeque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inicio"
        android:textColor="@color/azul_main"
        android:textSize="14dp"
        android:elevation="30dp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="705dp"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/logocitaspeque"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@android:color/transparent"
        android:elevation="30dp"
        android:src="@drawable/ic_calendar"
        android:tint="@color/azul_main"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="676dp"
        tools:ignore="MissingConstraints,UseAppTint" />
    <TextView
        android:id="@+id/citasPeque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Citas"
        android:textColor="@color/azul_main"
        android:textSize="14dp"
        android:elevation="30dp"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="705dp"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/logohistoriapeque"
        android:layout_width="30dp"
        android:layout_height="28dp"
        android:background="@android:color/transparent"
        android:elevation="30dp"
        android:src="@drawable/ic_hc"
        android:tint="@color/azul_main"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="679dp"
        tools:ignore="MissingConstraints,UseAppTint" />
    <TextView
        android:id="@+id/hcPeque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Historia"
        android:textColor="@color/azul_main"
        android:textSize="14dp"
        android:elevation="30dp"
        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="705dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/logoperfilpeque"
        android:layout_width="30dp"
        android:layout_height="28dp"
        android:background="@android:color/transparent"
        android:elevation="30dp"
        android:src="@drawable/ic_user"
        android:tint="@color/azul_main"
        tools:layout_editor_absoluteX="330dp"
        tools:layout_editor_absoluteY="679dp"
        tools:ignore="MissingConstraints,UseAppTint" />
    <TextView
        android:id="@+id/PerfilPeque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Perfil"
        android:textColor="@color/azul_main"
        android:textSize="14dp"
        android:elevation="30dp"
        tools:layout_editor_absoluteX="330dp"
        tools:layout_editor_absoluteY="705dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>